{
    "builds": [
        {
            "src": "wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "wsgi.py"
        }
    ],
    "functions": {
        "wsgi.py": {
            "runtime": "python3.9"
        }
    }
}