{
    "builds": [
        {
            "src": "vercel_app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/favicon.ico",
            "dest": "/staticfiles/favicon.ico"
        },
        {
            "src": "/(.*)",
            "dest": "vercel_app.py"
        }
    ],
    "functions": {
        "vercel_app.py": {
            "runtime": "python3.9"
        }
    },
    "buildCommand": "python vercel_build.py"
}